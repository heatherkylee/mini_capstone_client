<a href="http://localhost:3001/client/products/new">Insert New Product</a>
<% @products.each do |product| %>
  <p>name: <a href="/client/products/<%= product['id'] %>"><%= product['name'] %></a></p>
  <p>id: <%= product['id'] %></p>
  <p>price: <%= product['price'] %></p>
  <p>description: <%= product['description'] %></p>
  <p>image: <%= product['image'] %></p>
  <hr>
<% end %>